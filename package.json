{
  "name": "octopus-logs",
  "displayName": "Octopus Logs",
  "description": "Will display logs from Octopus Deploy",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "octopus-logs.viewLogs",
        "title": "Octopus: View Logs"
      },
      {
        "command": "octopus-logs.initOctopus",
        "title": "Octopus: Initialize"
      },
      {
        "command": "octopus-logs.deInitOctopus",
        "title": "Octopus: De-Initialize"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.66.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.11.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@octopusdeploy/api-client": "^1.1.7",
    "@octopusdeploy/message-contracts": "^1.1.1",
    "axios": "^0.26.1",
    "lodash": "^4.17.21",
    "semver": "^7.3.5",
    "urijs": "^1.19.10"
  }
}
